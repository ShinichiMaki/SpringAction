<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="compactDisc" class="com.spring.chapter2.xmlconfig.soundsystem.BlankDisc"
		c:title="Sgt. Pepper's Lonely Hearts Club Band" c:artist="The Beatles" />
	<!-- c:_0="Sgt. Pepper's Lonely Hearts Club Band" c:_1="The Beatles" -->
	<!-- c:title="Sgt. Pepper's Lonely Hearts Club Band" c:artist="The Beatles" -->

	<bean id="cdPlayer" class="com.spring.chapter2.xmlconfig.soundsystem.CDPlayer"
		c:_-ref="compactDisc" />

</beans>